{
  "manifest_version": 2,
  "name": "Downrawd",
  "version": "1.0",
  "description": "Download raw data on github",
  "icons" : {
    "64": "img/icon.png"
  },
  "page_action": {
    "default_icon": "img/icon.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "tabs"
  ]
}
